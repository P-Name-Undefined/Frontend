{
  "expo": {
    "name": "Mia Ajuda",
    "slug": "miaajuda",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4b8ab9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "notification": {
      "icon": "./assets/images/notificationIcon.png",
      "color": "#4b8ab9",
      "androidMode": "default"
    }
  }
}
