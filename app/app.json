{
    "expo": {
        "name": "Mia Ajuda",
        "slug": "miaajuda",
        "privacy": "public",
        "platforms": [
            "android",
            "ios"
        ],
        "notification": {
            "icon": "./assets/images/logo.png",
            "color": "#4b8ab9",
            "androidMode": "default"
        },
        "version": "3.0.2 Corgi",
        "orientation": "portrait",
        "icon": "./assets/images/logo.png",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#4b8ab9"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.miaajuda.app"
        },
        "packagerOpts": {
            "config": "metro.config.js",
            "sourceExts": [
                "expo.ts",
                "expo.tsx",
                "expo.js",
                "expo.jsx",
                "ts",
                "tsx",
                "js",
                "jsx",
                "json",
                "wasm",
                "svg"
            ]
        },
        "extra": {
            "eas": {
                "projectId": ""
            }
        }
    }
}